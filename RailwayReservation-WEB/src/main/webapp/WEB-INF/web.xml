<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>AddTrainServlet</servlet-name>
    <servlet-class>AddATrainServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddTrainServlet</servlet-name>
    <url-pattern>/addTrain</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AddTripServlet</servlet-name>
    <servlet-class>AddATripServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddTripServlet</servlet-name>
    <url-pattern>/addTrip</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>AddTicketServlet</servlet-name>
    <servlet-class>AddATicketServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>AddTicketServlet</servlet-name>
    <url-pattern>/addTicket</url-pattern>
  </servlet-mapping>

</web-app>
